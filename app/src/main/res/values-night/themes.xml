<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.BaseTheme" parent="Theme.Mvcexample"/>

    <style name="Theme.Mvcexample" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/dark_colorPrimary</item>
        <item name="colorPrimaryVariant">@color/dark_colorPrimary</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/primaryAccent_black</item>
        <item name="colorSecondaryVariant">@color/primaryAccent_black</item>
        <item name="colorOnSecondary">@color/primaryAccent_black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <item name="android:windowBackground">@color/windowBackgroundDark</item>


        <item name="StorkColor"> @color/primaryAccent_black</item>
    </style>
</resources>